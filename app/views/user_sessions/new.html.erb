<h1>Login</h1><% if @session.errors.has_key? :base %>   <div id="error_explanation">     <%= @session.errors[:base].join(', ') %>   </div><% end %><%= form_for @session do |f| %><p>	<%= f.label :cpf %>	<%= f.text_field :cpf %>	<%= error_tag @session, :pcpf %></p><p>	<%= f.label :senha %>	<%= f.text_field :senha %>		<%= error_tag @session, :senha %></p><p>	<%= f.submit %></p><% end %><p><%= link_to 'Register', new_usuario_path %></p>